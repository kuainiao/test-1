.checkbox {
	position: relative;
}
.checkbox i {
	cursor:pointer;
	position: absolute;
	bottom: -42px;
	left: 0px;
	display: block;
	width: 32px;
	height: 32px;
	outline: none;
	border: 2px solid #DDDDDD;
}
.checkbox input + i:after {
	position: absolute;
	opacity: 0;
}
.checkbox input + i:after {
	content: '';
	background: url("../images/img_sprite.png") no-repeat -41px -162px;
	top: 6px;
	left: 4px;
	width: 24px;
	height: 24px;
	font: normal 12px/16px FontAwesome;
	text-align: center;
	cursor: pointer;
}
.checkbox input:checked + i:after {
	opacity: 1;
}
.checkbox input {
	position: absolute;
	left: -9999px;
}
